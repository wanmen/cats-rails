<%= form_for [@rateable, @rate] do |f| %>
  <% if @rate.errors.any? %>
    <div class="error_messages">
      <h2>Please correct the following errors.</h2>
      <ul>
      <% @rate.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
  <div class="field">
    <%= f.label :star %><br>
    <%= f.number_field :star %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
